{
  "dependencies": {
    "markdown-it-container": "^4.0.0"
  },
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.3"
  },
  "scripts": {
    "build-html": "marp --engine engine.mjs --theme theme.css slide.md -o index.html",
    "build-pdf": "marp --engine engine.mjs --theme theme.css slide.md -o slide.pdf",
    "watch": "marp --engine engine.mjs --theme theme.css slide.md -o index.html -w"
  }
}